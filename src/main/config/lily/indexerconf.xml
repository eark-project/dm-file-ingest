<?xml version="1.0"?>

<indexer xmlns:eark="org.eu.eark">
  <recordFilter>
    <includes>
      <include recordType="eark:File" vtags="last"/>
    </includes>
  </recordFilter>

  <fields>
    <field name="path" value="eark:path"/>
    <field name="contentType" value="eark:contentType"/>
    <field name="size" value="eark:size"/>
    <field name="confidential" value="eark:confidential"/>
    <field name="content" value="eark:content" extractContent="true"/>
    <field name="package" value="eark:package"/>
    <field name="textCategory" value="eark:textCategory"/>
    <field name="eadid_s" value="eark:eadid_s"/>
    <field name="eadtitle_s" value="eark:eadtitle_s"/>
    <field name="eaddate_s" value="eark:eaddate_s"/>
    <field name="eaddatestructuredfrom_dt" value="eark:eaddatestructuredfrom_dt"/>
    <field name="eaddatestructuredto_dt" value="eark:eaddatestructuredto_dt"/>
    <field name="eadorigination_s" value="eark:eadorigination_s"/>
    <field name="eadabstract_t" value="eark:eadabstract_t"/>
    <field name="eadaccessrestrict_s" value="eark:eadaccessrestrict_s"/>
    <field name="eadclevel_s" value="eark:eadclevel_s"/>
  </fields>

</indexer>
